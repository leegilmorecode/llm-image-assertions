{
  "name": "llm-image-assertions",
  "version": "0.1.0-beta.2",
  "description": "A TypeScript library for testing AI generated images using AWS Bedrock",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json --listEmittedFiles",
    "postbuild": "rm tsconfig.build.tsbuildinfo",
    "build:watch": "tsc -w -p tsconfig.build.json",
    "clean": "rm -rf dist",
    "check": "npx @biomejs/biome check --write",
    "check:staged": "npx @biomejs/biome check --write --staged",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "release": "LEFTHOOK=0 commit-and-tag-version",
    "release:beta": "LEFTHOOK=0 commit-and-tag-version --prerelease beta"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.1",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.16",
    "commit-and-tag-version": "^12.5.1",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "jest": "^29.7.0",
    "lefthook": "^1.11.13",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock": "^3.812.0",
    "@aws-sdk/client-bedrock-runtime": "^3.812.0"
  },
  "keywords": ["CDK", "Jest", "test"],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
